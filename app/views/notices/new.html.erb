<h1>Post a New Notice</h1>

<%= form_with model: @notice, local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>

  <div>
    <%= form.submit 'Post Notice' %>
  </div>
<% end %>
